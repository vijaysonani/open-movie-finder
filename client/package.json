{
  "name": "open-movie-finder-client",
  "version": "0.0.1",
  "private": true,
  "description": "Open Movie Finder App",
  "repository": "https://github.com/vijaysonani/open-movie-finder",
  "license": "UNLICENSED",
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.18.0",
    "flux": "2.1.1",
    "jquery": "^3.3.1",
    "keymirror": "0.1.1",
    "object-assign": "4.1.1",
    "prop-types": "15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "^1.1.4",
    "semantic-ui-css": "^2.2.9",
    "semantic-ui-react": "^0.67.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "automock": true,
    "resetMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage/jest",
    "coverageReporters": [
      "cobertura"
    ],
    "coveragePathIgnorePatterns": [
      "ActionCreators.js",
      "WebAPIUtils.js"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules"
    ],
    "setupFiles": [
      "./helpers/jestsetup.js"
    ],
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$"
  }
}
